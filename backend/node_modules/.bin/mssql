#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/bin/node_modules:/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/node_modules:/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/mssql@11.0.1/node_modules:/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/bin/node_modules:/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/node_modules:/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/mssql@11.0.1/node_modules:/Users/ulasisibol/Desktop/PMCP/project-files/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mssql/bin/mssql" "$@"
else
  exec node  "$basedir/../mssql/bin/mssql" "$@"
fi
